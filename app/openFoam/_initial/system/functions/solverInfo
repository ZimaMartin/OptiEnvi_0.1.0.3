FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      solverInfo;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //



solverInfo
{
    type            solverInfo;
    libs            ("libutilityFunctionObjects.so");
    fields          (U p_rgh h k omega);
    writeResidualFields no;
}

